{
  "configurations": [
  {
    "name": "imtool-aos launcher",
    "type": "cppdbg",
    "request": "launch",
    "program": "${workspaceFolder}/build/imtool-aos/imtool-aos",
    "cwd": "${workspaceFolder}",
    "MIMode": "gdb",
    "args": ["I\\&O/input/deer-large.ppm", "I\\&O/output/output-vscodedebugger.ppm", "info"],
    "setupCommands": [
        {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        },
        {
            "description": "Set Disassembly Flavor to Intel",
            "text": "-gdb-set disassembly-flavor intel",
            "ignoreFailures": true
        }
    ]
  }
  ]
}